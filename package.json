  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "repository": "https://github.com/kalogs-c/format-google-cloud-yaml-envs-vscode",
  "activationEvents": [],
